<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.brosh.finance.monthlybudgetsync">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.InsertTransactionActivity"></activity>
        <activity android:name=".ui.TransactionsActivity" />
        <activity android:name=".ui.BudgetActivity" />
        <activity android:name=".ui.InitAppActivity" />
        <activity android:name=".login.Login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.Create_Budget_Activity" />
        <activity android:name=".login.Register"/>
        <activity android:name=".ui.MainActivity" />
    </application>

</manifest>